VERSION="1.33.1"
SRCDIR="libuv-v${VERSION}"
TARBALL="${SRCDIR}.tar.gz"
URL="https://dist.libuv.org/dist/v${VERSION}"
SHA256SUM="b4b5dc15103f7bbfecb81a0a9575841fdb7217b9f709634be8118972c1c8ce27"

prepare() {
	./autogen.sh
}

build() {
	run_configure "$1"
	make -j $NUMJOBS
}

deploy() {
	make DESTDIR="$SYSROOT" install
}
