VERSION="1.10"
SRCDIR="gzip-${VERSION}"
TARBALL="${SRCDIR}.tar.xz"
URL="https://ftp.gnu.org/gnu/gzip"
SHA256SUM="8425ccac99872d544d4310305f915f5ea81e04d0f437ef1a230dc9d1c819d7c0"

prepare() {
	return
}

build() {
	run_configure "$1" --with-regex=regcmp
	make -j $NUMJOBS
}

deploy() {
	make DESTDIR="$SYSROOT" install
}
